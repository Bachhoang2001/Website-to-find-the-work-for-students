@model PBL3.Models.Company.HomePageModel
@inject IHttpContextAccessor _httpContextAccessor
<div class='company-content'>
    <div class='company-page text-break'>
        <div class='headers'>
            <div class='headers__logo'>
                <div class='headers__logo__img'>
                    <picture>
                        <img alt="@Model.CompanyName" src="~/images/companies/@Model.AvtLocation" width="100">
                    </picture>
                </div>
            </div>
            <div class='headers__info'>
                <h1 class='headers__info__name'>
                    @Model.CompanyName
                </h1>
                <div class='headers__info__item'>
                    <div class='svg-icon svg-icon--grey headers__info__item-location'>
                        <i class="fa fa-location-dot"></i>
                        @{ string str = "| ";
                            foreach (var item in Model.ListCities)
                            {
                                str += item.CityName + " | ";
                            }
                            <div class='svg-icon__text'>@str</div>
                        }
                    </div>
                    <div class='svg-icon svg-icon--grey headers__info__item-location'>
                        <i class="fa fa-location"></i>
                        @{ str = "| ";
                            foreach (var item in Model.ListSkills)
                            {
                                str += item.SkillName + " | ";
                            }
                            <div class='svg-icon__text'>@str</div>
                        }
                    </div>
                    <div class='svg-icon svg-icon--grey headers__info__item-location'>
                        <i class="fa fa-star"></i>
                        @if (Model.Rate == 0)
                        {
                            <div class='svg-icon__text'>Chưa có đánh giá nào</div>
                        }
                        else
                        {
                            <div class='svg-icon__text'>@Model.Rate.ToString("0.0")</div>
                        }

                    </div>
                </div>
            </div>
            @if (_httpContextAccessor.HttpContext.Session.GetInt32(CommonConstraints.USER_ROLEID) == 1 ||
_httpContextAccessor.HttpContext.Session.GetInt32(CommonConstraints.USER_ROLEID) == 3)
            {
                <div class='headers__actions'>
                    <a id="delete-account" class="login-window button">
                        Đánh giá công ty
                    </a>
                </div>
                <dialog id="confirm-delete" class="site-dialog" style="width: 50%;border-radius:10px;">
                    <header class="dialog-header">
                        <h1>Đánh giá công ty @Model.CompanyName</h1>
                    </header>
                    <div style="display: flex;justify-content: space-around;align-items: center;">
                        <div class="dialog-content">
                            <form class="rating">
                                <label>
                                    <input type="radio" name="stars" value="1" />
                                    <span class="icon">★</span>
                                </label>
                                <label>
                                    <input type="radio" name="stars" value="2" />
                                    <span class="icon">★</span>
                                    <span class="icon">★</span>
                                </label>
                                <label>
                                    <input type="radio" name="stars" value="3" />
                                    <span class="icon">★</span>
                                    <span class="icon">★</span>
                                    <span class="icon">★</span>
                                </label>
                                <label>
                                    <input type="radio" name="stars" value="4" />
                                    <span class="icon">★</span>
                                    <span class="icon">★</span>
                                    <span class="icon">★</span>
                                    <span class="icon">★</span>
                                </label>
                                <label>
                                    <input type="radio" name="stars" value="5" />
                                    <span class="icon">★</span>
                                    <span class="icon">★</span>
                                    <span class="icon">★</span>
                                    <span class="icon">★</span>
                                    <span class="icon">★</span>
                                </label>
                            </form>
                        </div>
                        <div>
                            <p name="star1">1★: Rất Tệ</p>
                            <p name="star2">2★: Tệ</p>
                            <p name="star3">3★: Bình Thường</p>
                            <p name="star4">4★: Tốt</p>
                            <p name="star5">5★: Rất Tốt</p>
                        </div>
                    </div>
                    <div class="btn-group cf" style="width:100%">
                        <form style="width:50%;" id="RatingByStudent">
                            <button class="btn btn-danger" style="width:100%;" onclick="displayRadioValue('@Model.CompanyID')">Đánh giá</button>
                        </form>
                        <button class="btn btn-cancel" id="cancel">Hủy</button>
                    </div>
                </dialog>
            }
        </div>
        <ul class='nav bg-white pt-1 mt-3' style="border-radius: 3px;">
            <li class='nav-item m-0 py-0 px-3 p-md-0'>
                <a data-controller="utm-tracking" class="nav-link me-3 active" asp-controller="Company" asp-action="Index" asp-route-id="@Model.CompanyID" style="-webkit-text-stroke-width:thin; color:black;">Bài đăng đã duyệt</a>
            </li>
            @if (_httpContextAccessor.HttpContext.Session.GetInt32(CommonConstraints.USER_ROLEID) == 2 &&
                          _httpContextAccessor.HttpContext.Session.GetInt32(CommonConstraints.USER_ID) == Model.CompanyID)
            {
                <li class='nav-item m-0 py-0 px-3 p-md-0'>
                    <a data-controller="utm-tracking" class="nav-link me-3 active" asp-controller="Company" asp-action="PendingApproval" style="-webkit-text-stroke-width:thin; color:black;">Bài đăng chờ duyệt</a>
                </li>
            }
            <li class='nav-item m-0 py-0 px-3 p-md-0'>
                <a data-controller="utm-tracking" class="nav-link ma-3 review-tab" style="border-bottom: solid;-webkit-text-stroke-width: medium;border-color: cornflowerblue;">@Model.Company.CompanyFeedbacks.Count() Lượt đánh giá</a>
            </li>
        </ul>
        <div class='company-page__container'>
            <div class='company-page__left mt-1'>
                <div class='panel panel-default jobs'>
                    <div class='panel-heading'>
                        <h2 class='panel-title'>Danh sách lượt đánh giá.</h2>
                    </div>
                    <div class='panel-body'>
                        @{
                            foreach (var feedback in Model.Company.CompanyFeedbacks.Skip((Model.CurrentPage - 1) * Model.MaxPerPage).Take(Model.MaxPerPage).ToList())
                            {
                                <div class="job">
                                    <div class='job_content'>
                                        <div class='logo'>
                                            <div class='logo-wrapper job-details-link-wrapper'>
                                                <a target="_blank" href="/companies/template-net">
                                                    <picture>
                                                        <img alt="@feedback.Student.IdNavigation.AvtLocation" src="~/images/students/@feedback.Student.IdNavigation.AvtLocation" width="75" style="border-radius: 10px;width: 75px;">
                                                    </picture>
                                                </a>
                                            </div>
                                            <div class='clearfix'></div>
                                        </div>
                                        <div class='job__description'>
                                            <div class='job__body'>
                                                <div class='details'>
                                                    <h3 class='title job-details-link-wrapper'>
                                                        <a target="_self">@feedback.Student.IdNavigation.SubName @feedback.Student.IdNavigation.GivenName</a>
                                                    </h3>
                                                    <div class='svg-icon svg-icon--grey headers__info__item-location'>
                                                        <i class="fa fa-building icon"></i>
                                                        <div class='svg-icon__text'> Khoa: @feedback.Student.FacultyNavigation.FacultyName</div>
                                                    </div>
                                                    <div class='svg-icon svg-icon--grey headers__info__item-location'>
                                                        @if (feedback.Rating == 5)
                                                        {
                                                            <div class='svg-icon__text'>⭐⭐⭐⭐⭐</div>
                                                            <div class='svg-icon__text'>Rất tốt</div>
                                                        }
                                                        else if (feedback.Rating == 4)
                                                        {
                                                            <div class='svg-icon__text'>⭐⭐⭐⭐</div>
                                                            <div class='svg-icon__text'>Tốt</div>
                                                        }
                                                        else if (feedback.Rating == 3)
                                                        {
                                                            <div class='svg-icon__text'>⭐⭐⭐</div>
                                                            <div class='svg-icon__text'>Bình thường</div>
                                                        }
                                                        else if (feedback.Rating == 2)
                                                        {
                                                            <div class='svg-icon__text'>⭐⭐</div>
                                                            <div class='svg-icon__text'>Tệ</div>
                                                        }
                                                        else
                                                        {
                                                            <div class='svg-icon__text'>⭐</div>
                                                            <div class='svg-icon__text'>Rất tệ</div>
                                                        }
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class='city_and_posted_date d-none d-sm-flex'>
                                            <div class='distance-time-job-posted'>

                                                @{
                                                    TimeSpan ts = DateTime.Now - feedback.Updatedate;
                                                    int Total = (int)ts.TotalMinutes;
                                                    str = "";
                                                    if (Total > 2592000)
                                                    {
                                                        <span class='distance-time highlight'>
                                                            @feedback.Updatedate.ToString("HH:mm tt dd/MM/yyyy")
                                                        </span>
                                                    }
                                                    else
                                                    {
                                                        int d = Total / 1440; Total %= 1440;
                                                        int h = Total / 60; Total %= 60;
                                                        int m = Total;
                                                        if (d > 0) str += d + "d trước";
                                                        else if (h > 0) str += h + "h trước";
                                                        else str += m + "p trước";
                                                        <span class='distance-time highlight'>
                                                            @str
                                                        </span>
                                                    }
                                                }
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            }
                        }

                    </div>
                    <div style="display: flex;justify-content: center;">
                        @{
                            int TotalPage = Model.Company.CompanyFeedbacks.Count() / Model.MaxPerPage;
                            if (Model.Company.CompanyFeedbacks.Count() % Model.MaxPerPage > 0) TotalPage += 1;
                            for (int i = 0; i < TotalPage; i++)
                            {
                                if (i + 1 == Model.CurrentPage)
                                {
                                    <a style="-webkit-text-stroke: thick;">[@(i+1)]</a>
                                }
                                else
                                {
                                    <a style="-webkit-text-stroke: medium;" asp-controller="Company" asp-action="Reviews" asp-route-id="@Model.CompanyID" asp-route-currentpage="@(i+1)">[@(i+1)]</a>
                                }
                            }
                        }
                    </div>
                </div>


